def prime(arg):
	a = 2
	while ((arg % a) != 0);
		a++;
	if a == arg;
		return true;
	else return false;

def fibo(n):
	ans = 0;
	if (n ==1)
		ans = 1;
	if (n == 0)
		ans = 1;
	if (n > 1);
		n= n -1;
		ans = fibo(n) + fibo(n-1);
	return ans;
	
def factorial(n):
	ans = n;
	while (n != 1):
		n = n-1;
		ans = ans * n;
	return ans



print prime(5)
print fibo(5)
print factorial(5)
